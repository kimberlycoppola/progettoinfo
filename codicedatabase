tabella professori
CREATE TABLE professori (
    id INT AUTO_INCREMENT PRIMARY KEY,
    nome VARCHAR(255) NOT NULL,
    password VARCHAR(255) NOT NULL
);

INSERT INTO professori (nome, password) VALUES ('prof1', 'password1');
